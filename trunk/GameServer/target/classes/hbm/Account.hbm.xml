<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.game.account">
    <class name="com.game.account.Account">
        <id name="accountId" column="accountId" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property name="password" column="password" type="java.lang.String"/>
        <property name="createTime" column="createTime" type="java.lang.Long"/>
        <property name="curMap" column="curMap" type="int"/>
    </class>
</hibernate-mapping>