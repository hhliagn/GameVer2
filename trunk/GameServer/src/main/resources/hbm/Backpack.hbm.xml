<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.game.backpack">
    <class name="com.game.backpack.Backpack">
        <id name="accountId" column="accountId" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property name="capacity" column="capacity" type="int"/>
        <property name="itemsData" column="itemsData" type="byte[]"/>
    </class>
</hibernate-mapping>